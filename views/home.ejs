<div class = "gallery-image-container">
<div class="card-deck">
    <% images.forEach((e) => { %>
    <div class="card">
        <a href='/images/<%= e.id %>'><img class="card-img-top" src="<%= e.url %>" alt="Card image cap"/></a>
        <div class="card-body">
        <h5 class="card-title"><%= e.location %></h5>
        </div>    
    </div>
    <% }) %>
    </div>   
</div>

