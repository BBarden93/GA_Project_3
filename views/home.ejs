<% images.forEach((row) => { %>
    <div class="row">
        <% row.forEach((e) => { %>
            <div class="col-md-3">
                <div class="card">
                    <a href='/images/<%= e.id %>'><img class="card-img-top" src="<%= e.url %>" alt="Card image cap"/></a>
                    <div class="card-body">
                    <h5 class="card-title"><%= e.location %></h5>
                    </div>    
                </div>
            </div>
        <% }) %>
    </div>
<% }) %>
